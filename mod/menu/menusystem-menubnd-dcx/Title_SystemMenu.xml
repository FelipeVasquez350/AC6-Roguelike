	<!-- ↓以下は子ダイアログが無いので url=指定が必要 -->
	<!-- ↓【アプリケーション終了がある】↓ -->
	<!-- 空                   --><NullCmd display="menuFeature/option_appQuit" />
	<!-- アプリケーション終了 --><Cmd captionID="400090" url="menu/quitApp" autoHide="false" display="menuFeature/option_appQuit"/>
